# Evaluator Agent configuration
LOG_LEVEL=INFO

# Comma-separated list of Mongo connection URIs for each agent's MongoDB
MONGO_URIS=mongodb://mongo1:27017,mongodb://mongo2:27017

# Central PostgreSQL DSN
POSTGRES_DSN=postgresql://user:password@postgres:5432/ai_village

# Default agent id (optional)
DEFAULT_AGENT_ID=agent1

# Scheduler interval seconds
SCHEDULE_INTERVAL_SECONDS=60

# GPT-5 API configuration (optional)
GPT5_API_BASE=https://api.openai.com/v1
GPT5_API_KEY=
GPT5_MODEL=gpt-5-reasoning
